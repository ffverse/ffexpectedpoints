Package: ffexpectedpoints
Title: Models for Fantasy Football Expected Points
Version: 0.1.0.02
Authors@R: c(
    person("Joe", "Sydlowski", , "syd235@gmail.com", role = c("aut", "cre")),
    person("Tan", "Ho", , "tan@tanho.ca", role = "aut",
           comment = c(ORCID = "0000-0001-8388-5155"))
